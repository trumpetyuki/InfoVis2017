    <script src="vec3.js"></script>
    <script>
      var v1 = new Vec3(5,4,8);
      var v2 = new Vec3(2,1,7);
      var v = v1.add(v2);//v=(7,5,15)
      var sum = v.sum();//27=5+7+15
      document.write(sum);
    </script>
